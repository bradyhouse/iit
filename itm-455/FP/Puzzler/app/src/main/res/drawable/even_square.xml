<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#f2ebdd"
                android:endColor="#f2ebdd"
                android:angle="270" />
            <solid
                android:color="#f2ebdd" />
            <stroke
                android:width="1dp"
                android:color="#6b7781" />
            <corners
                android:radius="100dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#f2ebdd"
                android:endColor="#f2ebdd"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="#6b7781" />
            <corners
                android:radius="25dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>
